{
    "name": "twhiston/tg",
    "description": "Robo Task Runner, with added autoloading",
    "authors": [
        {
            "name": "Thomas Whiston",
            "email": "tom.whiston@gmail.com"
        }
    ],
    "require": {
        "twhiston/twLib": "^1.1",
        "symfony/yaml": "^3.1",
        "twig/twig": "^1.24"
    },
    "require-dev": {
        "codegyre/robo": "^0.7.2",
        "phpunit/phpunit": "^5.4",
        "twhiston/tg_lib_test": "^0.1.0"
    },
    "autoload": {
        "psr-4" : {
            "twhiston\\tg\\" : "src"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://twhiston@bitbucket.org/twhiston/tg-lib-test.git"
        }
    ]
}
